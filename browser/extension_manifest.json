{
    "manifest_version": 3,
    "name": "Media Processor",
    "version": "1.0",
    "description": "Send URLs to Media Processor desktop application",
    "permissions": [
        "activeTab",
        "contextMenus"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_title": "Send to Media Processor"
    },
    "icons": {
        "16": "data:image/svg+xml;base64,",
        "32": "data:image/svg+xml;base64,"
    }
}
